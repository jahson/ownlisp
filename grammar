decimal  : /-?[0-9]+\.[0-9]+/ ;
integer  : /-?[0-9]+/ ;
number   : <decimal> | <integer> ;
symbol   : '+' | '-' | '*' | '/' | '%' | '^' | "add" | "sub" | "mul" | "div" | "mod" | "pow" | "min" | "max" | "list" | "head" | "tail" | "join" | "eval" | "len" | "init" ;
sexpr    : '(' <expr>* ')' ;
qexpr    : '{' <expr>* '}' ;
expr     : <number> | <symbol> | <sexpr> | <qexpr> ;
lispy    : /^/ <expr>* /$/ ;
