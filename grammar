decimal  : /-?[0-9]+\.[0-9]+/ ;
integer  : /-?[0-9]+/ ;
number   : <decimal> | <integer> ;
symbol   : '+' | '-' | '*' | '/' | '%' | '^' | "add" | "sub" | "mul" | "div" | "mod" | "pow" | "min" | "max";
sexpr    : '(' <expr>* ')' ;
expr     : <number> | <symbol> | <sexpr>
lispy    : /^/ <expr>* /$/ ;
